{
    "es_template": {
        "template" : "logstash*"
        , "settings" : {
            "index.store.compress.stored" : true
        }
        , "mappings" : {
            "_default_": {
               "_all" : {"enabled" : false}
               , "date_detection" : false
               , "properties" : {
                  "@message": { "type": "string", "index": "no" }
                  , "@source": { "type": "string", "index": "no" }
                  , "@source_host": { "type": "string", "index": "no" }
                  , "@source_path": { "type": "string", "index": "no" }
                  , "@tags": { "type": "string", "index": "not_analyzed" }
                  , "@timestamp": { "type": "date", "index": "not_analyzed", "format" : "dateOptionalTime" }
                  , "@type": { "type": "string", "index": "no" }    
               }
            },
            "beer-search" : {
               "properties" : {
                  "@fields" : {
                       "type" : "object"
                       , "dynamic": false
                       , "properties": {
                            "user_cookie_id": { "type": "string", "index": "not_analyzed" }
                            , "http_status": { "type": "long", "index": "not_analyzed" }
                            , "hits": { "type": "long", "index": "not_analyzed" }
                            , "q": { "type": "string" }
                            , "results": { "type": "string", "index": "not_analyzed" }
                            , "error_msg": { "type" : "string", "index": "analyzed" }
                            , "bot_or_client": { "type" : "string", "index": "not_analyzed" }
                        }
                    }
                }   
            }
            , "beer-abs" : {
                "properties" : {
                    "@fields" : {
                        "type" : "object"
                        , "dynamic" : false
                        , "properties" : {
                            "user_cookie_id" : { "type": "string", "index": "not_analyzed" }
                            , "bibcode" : { "type": "string", "index": "not_analyzed" }
                            , "type" : { "type": "string", "index": "not_analyzed" }
                            , "bot_or_client": { "type" : "string", "index": "not_analyzed" }
                        }
                    }
                }
            }
            , "beer-api" : {
               "properties" : {
                  "@fields" : {
                       "type" : "object"
                       , "dynamic": false
                       , "properties": {
                            "dev_key": { "type": "string", "index": "not_analyzed" }
                            , "http_status": { "type": "long", "index": "not_analyzed" }
                            , "hits": { "type": "long", "index": "not_analyzed" }
                            , "q": { "type": "string" }
                            , "results": { "type": "string", "index": "not_analyzed" }
                        }
                    }
                }   
            }
            , "beer-logs" : {
                "dynamic_templates" : [
                    {
                        "template_1" : {
                            "path_match" : "@fields.qstring__*"
                            , "mapping" : {
                                "type": "string"
                                , "index" : "not_analyzed"
                            }
                        }
                    }
                    , {
                        "template_2" : {
                            "path_match" : "@fields.cookie__NASA*"
                            , "mapping" : {
                                "type": "string"
                                , "index" : "not_analyzed"
                            }
                        }
                    }
                    , {
                        "template_3" : {
                            "match" : "*"
                            , "mapping" : {
                                "type" : "string"
                                , "index" : "no"
                            }
                        }
                    }
                ]
                , "properties" : {
                    "@fields" : {
                       "type" : "object"
                       , "dynamic": true
                       , "properties": {
                            "http_status": { "type": "long", "index": "not_analyzed" }
                            , "bot_or_client": { "type" : "string", "index": "not_analyzed" }
                        }
                    }
                }   
            }
            , "classic-logs" : {
                "dynamic_templates" : [
                    {
                        "template_1" : {
                            "path_match" : "@fields.qstring__*"
                            , "mapping" : {
                                "type": "string"
                                , "index" : "not_analyzed"
                            }
                        }
                    }
                    , {
                        "template_2" : {
                            "path_match" : "@fields.cookie__NASA*"
                            , "mapping" : {
                                "type": "string"
                                , "index" : "not_analyzed"
                            }
                        }
                    }
                    , {
                        "template_3" : {
                            "match" : "*"
                            , "mapping" : {
                                "type" : "string"
                                , "index" : "no"
                            }
                        }
                    }
                ]
                , "properties" : {
                    "@fields" : {
                       "type" : "object"
                       , "dynamic": true
                       , "properties": {
                            "http_status": { "type": "long", "index": "not_analyzed" }
                        }
                    }
                }   
            }
        }
    }
}
